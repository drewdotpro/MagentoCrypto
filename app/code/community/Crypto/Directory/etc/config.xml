<config>
    <modules>
        <Crypto_Directory>
            <version>1.0.0</version>
        </Crypto_Directory>
    </modules>
    <global>
        <blocks>
            <cryptodirectory>
                <class>Crypto_Directory_Block</class>
            </cryptodirectory>
        </blocks>
        <models>
            <directory>
                <rewrite>
                    <currency>Crypto_Directory_Model_Currency</currency>
                </rewrite>
            </directory>
            <cryptodirectory>
                <class>Crypto_Directory_Model</class>
                <resourceModel>cryptodirectory_resource</resourceModel>
            </cryptodirectory>
            <cryptodirectory_resource>
                <class>Crypto_Directory_Model_Resource</class>
            </cryptodirectory_resource>
        </models>
        <resources>
            <cryptodirectory_setup>
                <setup>
                    <module>Crypto_Directory</module>
                </setup>
            </cryptodirectory_setup>
        </resources>
    </global>
</config>
